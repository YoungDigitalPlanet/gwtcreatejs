<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.4.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/2.4.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="gwtcreatejs">
    <inherits name="com.google.gwt.user.User"/>
    <inherits name="com.google.gwt.http.HTTP"/>
    <inherits name="com.google.gwt.xml.XML"/>
    <inherits name="eu.ydp.jsfilerequest.jsfilerequest"/>
    <inherits name="com.google.common.collect.Collect"/>

    <entry-point class='eu.ydp.gwtcreatejs.client.CreateJsEntryPoint'/>
    
    <source path="client"/>
</module>